{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Authority": "https://login.microsoftonline.com/{tenantID}/v2.0/",
    "TenantId": "{tenantID}",
    "ClientId": "{clientID}",
    "CallbackPath": "/signin-oidc"
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=webapp-oidc-sample;Trusted_Connection=True;MultipleActiveResultSets=true"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "AllowedHosts": "*"
}
